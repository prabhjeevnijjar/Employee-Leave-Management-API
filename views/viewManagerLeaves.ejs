<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <div class="topnav">
        <h1>Leave management</h1>
        <a href="/employeeHomepage">Go back</a>
      </div>
      <table>
        <thead>
          <tr>
            <th>Description</th>
            <th>Startdate</th>
            <th>Enddate</th>
            <th>Available leaves</th>
            <th>Status</th>

          </tr>
        </thead>
        <tbody>
          <%if (records.length >0){ %>
          <% records.forEach(function(row){ %>
          <tr>
            <td><%= row.description %></td>
            <td><%= row.startdate %></td>
            <td><%= row.enddate %></td>
            <td><%= row.leavesavailable %></td>
            <td><%= row.status %></td>
          </tr>
          <% })}else{ %>
            <tr>
                <td colspan="6">No Record Found</td>
                
              </tr>
            <% } %>
        </tbody>
      </table>

    </body>
    </html>